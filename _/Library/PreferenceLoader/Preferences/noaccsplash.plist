<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>NoAccSplash</string>
	<key>entry</key>
	<dict>
		<key>cell</key>
		<string>PSLinkCell</string>
		<key>icon</key>
		<string>noaccsplash.png</string>
		<key>label</key>
		<string>NoAccSplash</string>
	</dict>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Block Accessory Screen</string>
		</dict>
		<!-- Master Enable -->
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<real>1</real>
			<key>defaults</key>
			<string>com.truehybridx.noaccsplash</string>
			<key>key</key>
			<string>masterEnabled</string>
			<key>label</key>
			<string>Enabled</string>
		</dict>
		<!-- Video Enable Text First-->
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Display Video on Device</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<real>1</real>
			<key>defaults</key>
			<string>com.truehybridx.noaccsplash</string>
			<key>key</key>
			<string>videoEnable</string>
			<key>label</key>
			<string>Enable</string>
		</dict>
		<!-- Closing Text-->
		<dict>
	      <key>cell</key>
	      <string>PSGroupCell</string>
	      <key>footerText</key>
	      <string>If wanting Video Out, turn Device Video Off.
		
Release Info can be found at http://leimobile.com/noaccsplash-info-page/
Donations are accepted.</string>
	    </dict>
	</array>
</dict>
</plist>